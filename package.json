{
  "name": "mathjax-full",
  "version": "4.0.0-beta.1",
  "description": "Beautiful and accessible math in all browsers. MathJax is an open-source JavaScript display engine for LaTeX, MathML, and AsciiMath notation that works in all browsers and in server-side node applications. This package includes the source code as well as the packaged components.",
  "license": "Apache-2.0",
  "type": "module",
  "main": "components/src/node-main/node-main.js",
  "imports": {
    "#root/*": "mathjax-full/js6/components/mjs/*",
    "#mml3/*": "mathjax-full/js6/input/mathml/mml3/mjs/*",
    "#menu/*": "mj-context-menu/js/*",
    "#sre/*": "speech-rule-engine/mjs/*",
    "#mhchem/*": "mhchemparser/esm/*",
    "#default-font/*": "mathjax-modern-font/js6/*"
  },
  "files": [
    "/es6",
    "/es5",
    "/js6",
    "/js5",
    "/ts",
    "/components",
    "LICENSE",
    "README.md",
    "CONTRIBUTING.md",
    "tsconfig-*.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mathjax/Mathjax-src/"
  },
  "keywords": [
    "MathJax",
    "math",
    "svg",
    "HTML",
    "MathML",
    "TeX",
    "AsciiMath"
  ],
  "scripts": {
    "clean:dir": "clean() { npx rimraf $1; }; clean",
    "clean:lib": "npx rimraf 'components/src/**/lib'",
    "clean:n": "clean() { npm run -s clean:dir js$1 && npm run -s clean:dir es$1 && npm run -s clean:lib; }; clean",
    "clean": "npm run -s clean:n 5 && npm run -s clean:n 6",
    "=============================================================================== copy": "",
    "copy:mj2": "copy() { npx copyfiles -u 1 'ts/input/asciimath/legacy/**/*' js$1; }; copy",
    "copy:mml3": "copy() { npx copyfiles -u 1 ts/input/mathml/mml3/mml3.sef.json js$1; }; copy",
    "copy:pkg": "npx copyfiles -u 2 components/bin/package.json",
    "=============================================================================== compile": "",
    "_js:pkg5": "npm run -s copy:pkg js5",
    "_pre_compile": "pre() { npm run -s clean:dir js$1; }; pre",
    "_compile": "comp() { npx tsc --project tsconfig-es$1.json; }; comp",
    "_post_compile": "post() { npm run -s copy:mj2 $1 && npm run -s copy:mml3 $1 && npm run -s _js:pkg$1; }; post",
    "_compile_": "comp() { npm run -s _pre_compile $1; npm run -s _compile $1; npm run _post_compile $1; }; comp",
    "compile5": "npm run -s _compile_ 5",
    "compile6": "npm run -s _compile_ 6",
    "compile": "npm run compile6",
    "=============================================================================== components": "",
    "make-one": "make() { components/bin/makeAll --es$2 --no-subdirs components/src/$1; }; make",
    "_es:pkg5": "npm run -s copy:pkg es5",
    "_pre_make": "pre() { npm run -s clean:dir es$1 && npm run -s clean:lib; }; pre",
    "_make": "make() { components/bin/makeAll --es$1 --terse components/src; }; make",
    "_make_": "make() { npm run -s _pre_make $1 && npm run -s _make $1; npm run -s _es:pkg$1; }; make",
    "make-components5": "npm run -s _make_ 5",
    "make-components6": "npm run -s _make_ 6",
    "make-components": "npm run -s make-components6",
    "=============================================================================== build": "",
    "pre_build": "echo 'Compiling typescript files'",
    "_build": "build() { npm run -s compile$1; echo; npm run -s make-components$1; }; build",
    "build5": "echo '============='; echo 'Building ES5:'; echo '============='; npm run -s _build 5",
    "build6": "echo '============='; echo 'Building ES6:'; echo '============='; npm run -s _build 6",
    "build": "npm run -s build6",
    "build-all": "npm run -s build6 ; echo ; npm run -s build5 ; npm run -s use-es6",
    "=============================================================================== mml3": "",
    "premake-mml3-xslt": "cd ts/input/mathml/mml3 && grep '^\\s*\\(<\\|or\\|xmlns\\|excl\\|\">\\)' mml3.ts > mml3.xsl",
    "make-mml3-xslt": "cd ts/input/mathml/mml3 && npx xslt3 -t -xsl:mml3.xsl -export:mml3.sef.json -nogo",
    "postmake-mml3-xslt": "npx rimraf ts/input/mathml/mml3/mml3.xsl",
    "=============================================================================== install": "",
    "link:full": "node -e 'require(\"fs\").symlinkSync(process.cwd(), \"node_modules/mathjax-full\")'",
    "install": "npm run -s link:full; npm run -s use-es6",
    "use-es5": "node components/bin/use-es5 true",
    "use-es6": "node components/bin/use-es5 false"
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/preset-env": "^7.17.12",
    "babel-loader": "^8.2.5",
    "copyfiles": "^2.4.1",
    "diff": "^5.0.0",
    "rimraf": "^3.0.2",
    "tape": "^5.5.3",
    "terser-webpack-plugin": "^5.3.1",
    "tslint": "^6.1.3",
    "tslint-jsdoc-rules": "^0.2.0",
    "tslint-unix-formatter": "^0.2.0",
    "typescript": "^4.6.4",
    "typescript-tools": "^0.3.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "esm": "^3.2.25",
    "mathjax-modern-font": "^1.0.0-beta.3",
    "mhchemparser": "^4.2.0",
    "mj-context-menu": "^0.8.1",
    "speech-rule-engine": "^4.1.0-beta.4"
  }
}
